_simple_minus:
	ai	= 1
	t0	= -ai
	bi	= t0
	as	= 1
	t1	= -as
	bs	= t1
	ad	= 1
	t2	= -ad
	bd	= t2
	call	printf ("target -1: %d\n",bi)
	call	printf ("target -1: %d\n",bs)
	call	printf ("target -1.0: %f\n",bd)
_mix_minus:
	ai	= 1
	as	= 1
	ad	= 1
	t3	= -as
	bi	= t3
	t4	= -ad
	bs	= t4
	t5	= -ai
	bd	= t5
	call	printf ("target -1: %d\n",bi)
	call	printf ("target -1: %d\n",bs)
	call	printf ("target -1.0: %f\n",bd)
_main:
	call	simple_minus ()
	call	mix_minus ()
	call	getchar ()
